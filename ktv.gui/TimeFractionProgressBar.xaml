<UserControl x:Class="d9.ktv.gui.TimeFractionProgressBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:d9.ktv.gui"
             mc:Ignorable="d" 
             d:DesignHeight="28" d:DesignWidth="800">
    <DockPanel>
        <Label x:Name="TimeElapsedLabel" DockPanel.Dock="Left" Width="80" HorizontalContentAlignment="Left">0:00</Label>
        <Label x:Name="TimePerUpdateLabel" DockPanel.Dock="Right" Width="80" HorizontalContentAlignment="Right">0:00</Label>
        <ProgressBar x:Name="ProgressBar" Maximum="1"/>
    </DockPanel>
</UserControl>
